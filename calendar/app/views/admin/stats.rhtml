        <tr><td>num all day plans</td><td><%= @num_all_day_plans %></td></tr>
        <tr><td>num eve plans</td><td><%= @num_eve_plans %></td></tr>
        <tr><td>num dinner afternoon plans</td><td><%= @num_dinner_afternoon_plans %></td></tr>
        <tr><td>num lunch bfst plans</td><td><%= @num_lunch_bfst_plans %></td></tr>
        <tr><td>num morning plans</td><td><%= @num_morning_plans %></td></tr>
        <tr><td>num other time plans</td><td><%= @num_other_time_plans %></td></tr>
        
        <tr><td>users under 15</td><td><%= @users_0_to_14 %></td></tr>
        <tr><td>users 15 to 18</td><td><%= @users_15_to_18 %></td></tr>
        <tr><td>users 19 to 22</td><td><%= @users_19_to_22 %></td></tr>
        <tr><td>users 23 to 27</td><td><%= @users_23_to_27 %></td></tr>
        <tr><td>users 28 to 35</td><td><%= @users_28_to_35 %></td></tr>
        <tr><td>users 36 to 49</td><td><%= @users_36_to_49 %></td></tr>
        <tr><td>users 50 to 64</td><td><%= @users_50_to_64 %></td></tr>
        <tr><td>users over 64</td><td><%= @users_65_to_200 %></td></tr>
        <tr><td>users with no age</td><td><%= @users_no_age %></td></tr>
        
        <tr><td>male users</td><td><%= @male_users %></td></tr>
        <tr><td>female users</td><td><%= @female_users %></td></tr>
        <tr><td>users with unknown gender</td><td><%= @gender_unknown_users %></td></tr>
        <tr><td>users with unset gender</td><td><%= @no_gender_users %></td></tr>
        
        <tr><td>single users</td><td><%= @single_users %></td></tr>
        <tr><td>taken users</td><td><%= @taken_users %></td></tr>
        <tr><td>relationship unknown users</td><td><%= @relationship_unknown_users %></td></tr>
        <tr><td>relationship no data users</td><td><%= @relationship_no_data_users %></td></tr>
        
<%
@zip_1_data.each do | row | %>

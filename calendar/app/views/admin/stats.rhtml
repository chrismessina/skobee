<div id="banner">
  <h2 id="user_heading">
     Statistics
  </h2>
</div>

<div id="container" class="no_sidebar settings_container">
    <table cellpadding="0" cellspacing="0" border="1" class="statistics" summary="statistics">
        <tr><td>sessions</td><td><%= @num_sessions %></td></tr>
        <tr><td>users</td><td><%= @num_users %></td></tr>
        <tr><td>registered users</td><td><%= @num_registered_users %></td></tr>
        <tr><td>users added by existing members</td><td><%= @num_users_added_by_existing_members %></td></tr>
        <tr><td>avg plans per user</td><td><%= @avg_plans_per_user %></td></tr>
        <tr><td>avg contacts per user</td><td><%= @avg_contacts_per_user %></td></tr>
        <tr><td>avg checked contacts per user</td><td><%= @avg_checked_contacts_per_user %></td></tr>
        <tr><td>num users not notified on plan mod</td><td><%= @num_users_not_notified_on_plan_mod %></td></tr>
        <tr><td>num users not notified on invite</td><td><%= @num_users_not_notified_on_invite %></td></tr>
        <tr><td>plans</td><td><%= @num_plans %></td></tr>
